<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2010/07/nuspec.xsd">
  <metadata>
    <id>Anvil</id>
    <version>0.0.0.2</version>
    <title>Anvil</title>
    <authors>Garrett Pauls</authors>
